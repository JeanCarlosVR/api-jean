<style>
    body {
        max-width: 100%;
        font-family: 'Quicksand', sans-serif;
        overflow: hidden;
        margin: 0;
    }

    .main-index-001 .ocean { 
    height: 5%;
    width:100%;
    position:absolute;
    bottom:0;
    left:0;
    }

    .main-index-001 .wave {
    background: url(https://s3-us-west-2.amazonaws.com/s.cdpn.io/85486/wave.svg) repeat-x; 
    position: absolute;
    top: -98px;
    width: 6400px;
    height: 198px;
    animation: wave 7s cubic-bezier( 0.36, 0.45, 0.63, 0.53) infinite;
    transform: translate3d(0, 0, 0);
    }

    .main-index-001 .wave:nth-of-type(2) {
    top: -65px;
    animation: wave 7s cubic-bezier( 0.36, 0.105, 0.63, 0.53) -.125s infinite, swell 7s ease -1.25s infinite;
    opacity: 1;
    }

    .main-index-001 .wave:nth-of-type(3) {
    top: -45px;
    animation: wave 7s cubic-bezier( 0.36, 0.85, 0.63, 0.53) -.125s infinite, swell 7s ease -1.25s infinite;
    opacity: 1;
    }

    .main-index-001 .wave:nth-of-type(4) {
    top: -35px;
    animation: wave 7s cubic-bezier( 0.36, 0.15, 0.63, 0.53) -.125s infinite, swell 7s ease -1.25s infinite;
    opacity: 1;
    }

    .main-index-001 .wave:nth-of-type(5) {
    top: -20px;
    animation: wave 7s cubic-bezier( 0.36, 0.5, 0.63, 0.53) -.125s infinite, swell 7s ease -1.25s infinite;
    opacity: 1;
    }

    @keyframes wave {
    0% {
        margin-left: 0;
    }
    100% {
        margin-left: -1600px;
    }
    }

    @keyframes swell {
    0%, 100% {
        transform: translate3d(0,-25px,0);
    }
    50% {
        transform: translate3d(0,5px,0);
    }
    }

</style>